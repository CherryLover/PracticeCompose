<network-security-config>
    <base-config>
        <trust-anchors>
            <certificates src="system" />  <!-- 信任系统自带的证书 -->
            <certificates src="user" />
        </trust-anchors>
    </base-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">aliyuncs.com</domain>
    </domain-config>
</network-security-config>